generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Registration {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  mobile      String
  role        String
  city        String
  description String?
  createdAt   DateTime @default(now())
}
